// mathfun.cpp

#include <iostream>
#include <time.h>
#include <cstdlib>

using namespace std;

extern "C" int product (int, int);
//extern "C" int power (int, int);

int  main () {

    // delcare the local variables
    int  x, y, prodResult;
    //int prowResult;
    cout << "Please the first integer:  ";
    cin >> x;

    cout << "Please the second integer:  ";
    cin >> y;

    prodResult = product(x, y);
    //powResult = power(x, y);
    cout << "The product is " << prodResult << endl;
    //cout << x << " to the power of " << y << " is " << endl;
    // all done!
    return 0;
}
